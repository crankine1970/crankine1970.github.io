        const user = {
            name: 'Claudette Rankine',
            imageUrl: ''https://github.com/crankine1970/crankine1970.github.io/blob/rankine-final/Favicon.ico',
            bio: ''The desktop performance audit of my GitHub page that was done via the Chrome Lighthouse audit tool returned an overall performance rating of 96 which is good. Accessibility and Best Practices both scored 100. SEO is 66 which needs improvement.

Diagnostic suggests:
Eliminate render-blocking resources Potential savings of 280 ms 
Reduce unused CSS Potential savings of 102 KiB
Page prevented back/forward cache restoration 2 failure reasons
Serve images in next-gen formats Potential savings of 10 KiB
'
        };

        // Step one - Create a React element
         class App extends React.Component {
  state = {
    count: 0,
  };
  handleClick = () => {
    this.setState((previousCount) => ({
      count: previousCount.count + 1,
    }));
  };

        // Step 2: Render the React element
             render() {
    return (
      <div>
        <h1>Click Me React Rendering</h1>
        <button onClick={this.handleClick}>Click me</button>
        <p>Button has been clicked {this.state.count} times</p>
      </div>
      <footer>
        <div id="Web Design Finals"></div>
      </footer>
   );
  }
}

React.render(<App />, document.getElementById('app'));

    </script>

